  inherited Q_padrao: TFDQuery
    SQL.Strings = (
      'SELECT '
      'A.VENDA_ID,'
      'A.CLIENTE_ID,'
      'B.NOME,'
      'B.CPF'
      'FROM VENDAS A'
      'INNER JOIN CLIENTE B ON B.CLIENTE_ID = A.CLIENTE_ID'
      'ORDER BY A.VENDA_ID')
    object Q_padraoVENDA_ID: TIntegerField
      FieldName = 'VENDA_ID'
      Origin = 'VENDA_ID'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object Q_padraoCLIENTE_ID: TIntegerField
      FieldName = 'CLIENTE_ID'
      Origin = 'CLIENTE_ID'
      Required = True
    end
    object Q_padraoNOME: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'NOME'
      Origin = 'NOME'
      ProviderFlags = []
      ReadOnly = True
      Size = 100
    end
    object Q_padraoCPF: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'CPF'
      Origin = 'CPF'
      ProviderFlags = []
      ReadOnly = True
      Size = 16
    end
  end
  object Q_Receber: TFDQuery
    IndexFieldNames = 'VENDA_ID'
    MasterSource = ds_padrao
    MasterFields = 'VENDA_ID'
    DetailFields = 'VENDA_ID'
    Connection = DM.conexao
    SQL.Strings = (
      'SELECT SEQUENCIA_ID,'
      'VENDA_ID,'
      'VALOR_PARCELA,'
      'DT_VENCIMENTO,'
      'DT_PAGAMENTO,'
      'ATRASO,'
      'JUROS,'
      'VL_JUROS,'
      'TOTAL_PAGAR,'
      'STATUS FROM CONTAS_RECEBER'
      'WHERE VENDA_ID=:VENDA_ID'
      'ORDER BY VENDA_ID')
    Left = 744
    Top = 200
    ParamData = <
      item
        Name = 'VENDA_ID'
        DataType = ftInteger
        ParamType = ptInput
