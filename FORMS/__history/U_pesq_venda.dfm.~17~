  inherited Q_pesq_padrao: TFDQuery
    Active = True
    SQL.Strings = (
      'SELECT '
      'A.VENDA_ID,'
      'A.CLIENTE_ID,'
      'B.NOME,'
      'A.ID_FORMA_PGTO,'
      'C.DESCRICAO,'
      'A.USUARIO,'
      'A.CADASTRO,'
      'A.VALOR'
      'FROM VENDAS A, CLIENTE B, FORMA_PGTO C '
      'WHERE A.CLIENTE_ID=B.CLIENTE_ID '
      'AND A.ID_FORMA_PGTO=C.ID_FORMA_PGTO'
      'ORDER BY A.VENDA_ID')
    object Q_pesq_padraoVENDA_ID: TIntegerField
      FieldName = 'VENDA_ID'
