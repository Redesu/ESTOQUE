  inherited Q_pesq_padrao: TFDQuery
    SQL.Strings = (
      'SELECT '
      'A.VENDA_ID,'
      'A.CLIENTE_ID,'
      'B.NOME,'
      'B.CPF,'
      'C.SEQUENCIA_ID,'
      'C.VALOR_PARCELA,'
      'C.DT_VENCIMENTO,'
      'C.DT_PAGAMENTO,'
      'C.ATRASO,'
      'C.JUROS,'
      'C.VL_JUROS,'
      'C.TOTAL_PAGAR,'
      'C.STATUS '
      'FROM VENDAS A'
      'INNER JOIN CLIENTE B ON B.CLIENTE_ID = A.CLIENTE_ID'
      'INNER JOIN CONTAS_RECEBER C ON C.VENDA_ID = A.VENDA_ID'
      'ORDER BY A.VENDA_ID')
