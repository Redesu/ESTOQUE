  object Q_conta_pagar: TFDQuery
    Connection = DM.conexao
    SQL.Strings = (
      'SELECT'
      ''
      'EXTRACT(MONTH FROM A.DT_PAGAMENTO) AS MES,'
      'EXTRACT(YEAR FROM A.DT_PAGAMENTO) AS ANO,'
      'SUM(A.TOTAL_PAGAR) AS TOTAL_PAGO'
      'FROM CONTAS_PAGAR A'
