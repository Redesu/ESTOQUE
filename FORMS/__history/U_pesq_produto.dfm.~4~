  inherited Q_pesq_padrao: TFDQuery
    SQL.Strings = (
      'SELECT A.PRODUTO_ID,'
      'A.PRODUTO_DESCRICAO,'
      'A.VL_CUSTO,'
      'A.VL_VENDA,'
      'A.ESTOQUE,'
      'A.ESTOQUE_MIN,'
      'A.UNIDADE,'
      'A.FORNECEDOR_ID,'
      'B.NOME,'
      'A.CADASTRO FROM PRODUTO A'
      'INNER JOIN FORNECEDOR B ON B.FORNECEDOR_ID = A.FORNECEDOR_ID')
